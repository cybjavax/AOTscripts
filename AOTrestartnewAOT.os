Функция ОбернутьВКавычки(Знач Строка);
	Если Лев(Строка, 1) = """" и Прав(Строка, 1) = """" Тогда
		Возврат Строка;
	Иначе
		Возврат """" + Строка + """";
	КонецЕсли;
КонецФункции

_каталогАОТ = "C:\1Cbases\AOT"; 
_пустаяAOT = "C:\1Cbases\AOTcf\AOT.dt";

